{
  "name": "vm0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "dev:tunnel": "bash scripts/dev-with-tunnel.sh",
    "lint": "turbo run lint",
    "format": "prettier --write --ignore-unknown .",
    "check-types": "turbo run check-types",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "knip": "knip",
    "knip:fix": "knip --fix --allow-remove-files",
    "knip:production": "knip --production --strict",
    "generate-certs": "bash ../scripts/generate-certs.sh",
    "check-certs": "node packages/proxy/scripts/check-certs.js",
    "e2b:build": "npx tsx ../e2b/build.prod.ts",
    "e2b:build:dev": "npx tsx ../e2b/build.dev.ts"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "^3.2.4",
    "knip": "^5.63.1",
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2",
    "vitest": "^3.2.4"
  },
  "packageManager": "pnpm@10.15.0",
  "engines": {
    "node": ">=20"
  }
}
